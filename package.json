{
  "name": "diffhtml",
  "version": "0.9.2",
  "description": "Easily swap out markup with an intelligent virtual dom",
  "main": "dist/diffhtml",
  "author": "Tim Branyen (@tbranyen)",
  "repository": "https://github.com/tbranyen/diffhtml",
  "license": "MIT",
  "scripts": {
    "prepublish": "babel lib --source-maps -d dist/cjs",
    "build": "npm run build-main && npm run build-runtime",
    "min": "npm run build-main-min && npm run build-runtime-min",
    "build-main": "rollup -c rollup.config.js",
    "build-runtime": "rollup -c rollup.runtime.config.js",
    "build-main-min": "uglifyjs dist/diffhtml.js -o dist/diffhtml.min.js",
    "build-runtime-min": "uglifyjs dist/diffhtml-runtime.js -o dist/diffhtml-runtime.min.js",
    "jshint": "jshint lib/**/*.js",
    "karma": "karma start",
    "test": "npm run jshint && npm run karma -- --single-run"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "coveralls": "^2.11.12",
    "derequire": "^2.0.3",
    "jshint": "^2.9.3",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-browserstack-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "sinon": "^1.17.6",
    "uglify-js": "mishoo/UglifyJS2#harmony",
    "watchify": "^3.7.0"
  }
}
