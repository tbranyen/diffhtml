{
  "name": "diffhtml-static-sync",
  "private": true,
  "bin": "index.js",
  "main": "index.js",
  "version": "1.0.0-beta.29",
  "description": "A static HTTP server that hot reloads HTML on save",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "mkdir -p dist ; browserify -t babelify sync.js -o dist/sync.js",
    "watch": "mkdir -p dist ; watchify -t babelify sync.js -o dist/sync.js -v"
  },
  "keywords": [
    "diffhtml",
    "html",
    "sync"
  ],
  "author": "Tim Branyen (@tbranyen)",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "cli-spinner": "^0.2.10",
    "diffhtml": "^1.0.0-beta.29",
    "engine.io": "^6.4.2",
    "express": "^4.17.3",
    "marked": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "engine.io-client": "^6.1.1",
    "watchify": "^4.0.0"
  }
}
