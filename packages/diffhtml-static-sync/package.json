{
  "name": "diffhtml-static-sync",
  "bin": "index.js",
  "main": "index.js",
  "version": "1.0.0-beta.18",
  "description": "A static HTTP server that hot reloads HTML on save",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "mkdir -p dist ; browserify -t babelify sync.js -o dist/sync.js",
    "watch": "mkdir -p dist ; watchify -t babelify sync.js -o dist/sync.js -v"
  },
  "keywords": [
    "diffhtml",
    "html",
    "sync"
  ],
  "author": "Tim Branyen (@tbranyen)",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.0",
    "cli-spinner": "^0.2.10",
    "diffhtml": "^1.0.0-beta.18",
    "engine.io": "^4.0.6",
    "express": "^4.17.1",
    "marked": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "engine.io-client": "^4.0.6",
    "watchify": "^3.11.1"
  }
}
